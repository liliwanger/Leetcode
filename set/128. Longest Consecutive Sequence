class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        #去重，O(1) 查找
        num_set = set(nums) # O(n)
        longest = 0

        for x in num_set:
            if x -1 not in num_set:
                #x 是起点
                current = x
                length =1
                while current + length in num_set:
                    length += 1
                    current += 1
                longest = max(longest, length)
        return longest
                
